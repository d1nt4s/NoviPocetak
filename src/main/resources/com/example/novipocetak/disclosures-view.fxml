<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox spacing="15" alignment="TOP_CENTER" xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx"
      fx:controller="com.example.novipocetak.controllers.DisclosureSessionsController">

    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Label text="Pregled seansi i testova" style="-fx-font-size: 18px; -fx-font-weight: bold"/>

    <TableView fx:id="seansaTable" prefHeight="400">
        <columns>
            <TableColumn fx:id="seansaIdCol" text="ID Seanse"/>
            <TableColumn fx:id="datumCol" text="Datum"/>
            <TableColumn fx:id="vremeCol" text="Vreme"/>
            <TableColumn fx:id="trajanjeCol" text="Trajanje (min)"/>
            <TableColumn fx:id="beleskeCol" text="BeleÅ¡ke"/>
            <TableColumn fx:id="testCol" text="Test"/>
            <TableColumn fx:id="rezultatCol" text="Rezultat"/>
            <TableColumn fx:id="objavaCol" text="Objavljeno (komu, razlog)"/>
            <TableColumn fx:id="klijentCol" text="Klijent"/>
        </columns>
    </TableView>

    <Button text="Povratak u meni" onAction="#openTherapeutMenu"/>
</VBox>
